{
  "expo": {
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/images/prulifeIcon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "visible": "leanback",
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "name": "Prulife UK Insurance",
    "slug": "prulife-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/prulifeIcon2.png",
    "scheme": "prulifemobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prulife.firebase",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "edgeToEdgeEnabled": false,
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": "visible",
        "barStyle": "light",
        "backgroundColor": "#000000",
        "gestureDirection": "none",
        "translucent": false
      },
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "red",
        "translucent": false
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png",
        "bundleIdentifier": "com.prulife.firebase"
      },
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "com.prulife.firebase"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/prulifeLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/prulifeLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "52229f7f-6959-47c5-bc8b-6bcb17ddaa94"
      }
    }
  }
}
